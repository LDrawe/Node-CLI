{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": [
        "standard"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {},
    "overrides": [
        {
            "files": [
                "**/*.spec.ts"
            ],
            "env": {
                "jest": true
            }
        }
    ]
}